{
  "name": "mumbai-dao-backend",
  "version": "1.0.0",
  "description": "Backend for Mumbai DAO",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "build:frontend": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install",
    "deploy": "npm run install:frontend && npm run build:frontend && npm start",
    "seed": "node scripts/seed.js"
  },
  "keywords": [
    "dao",
    "ethereum",
    "web3"
  ],
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ethers": "^6.13.7",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.14.0",
    "moongoose": "^0.0.5",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
